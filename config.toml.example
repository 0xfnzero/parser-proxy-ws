# Parser Proxy WebSocket 配置文件 (完整版)

[server]
host = "127.0.0.1"
port = 9001

[grpc]
endpoint = "https://solana-yellowstone-grpc.publicnode.com:443"
token = ""
enable_metrics = true
enable_tls = true
connection_timeout_ms = 10000
request_timeout_ms = 30000

[protocols]
pumpfun = true
pumpswap = false
bonk = false
raydium_amm_v4 = false
raydium_clmm = false
raydium_cpmm = false

[events]
# Block 事件
block_meta = false

# Bonk 事件
bonk_trade = false
bonk_pool_create = false
bonk_migrate_amm = false

# PumpFun 事件
pumpfun_trade = true
pumpfun_create = true
pumpfun_complete = false
pumpfun_migrate = false

# PumpSwap 事件
